{
  "defaultProject": "core",
  "projects": {
    "core": {
      "timezones": [
        "America/Los_Angeles"
      ],
      "currencies": [
        "USD"
      ],
      "interface": {
        "autoOpen": true,
        "port": 3001
      },
      "languages": {
